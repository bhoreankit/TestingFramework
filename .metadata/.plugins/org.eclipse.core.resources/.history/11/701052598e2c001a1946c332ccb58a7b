package Package1;

public class CountCharacterOccurence1 {

	public static void main(String[] args) {
		
		String s=new String("I am ankit bhore akola ");
		char ch='a';
		int count=0;
		for (int i = 0; i < s.length(); i++) {
			if(ch==s.charAt(i)) {
				count++;
			}
		}
		System.out.println(count);
	}

}
