package Package1;

public class CountCharacterOccurence {
	
	public static void main(String[] args)
    {
        String s = "Java is java again java again";
        
        char c='a';
        int count=0;
        
        for (int i = 0; i < s.length(); i++) {
        	
        	if(s.charAt(i)) {
        		count++;
        	}
			
		}
        System.out.println(count);
       
    }

}
